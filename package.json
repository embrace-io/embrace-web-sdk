{
  "name": "@embraceio/embrace-web-sdk",
  "version": "0.0.8",
  "description": "Embrace Web SDK",
  "type": "module",
  "main": "build/src/index.js",
  "module": "build/esm/index.js",
  "esnext": "build/esnext/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run sdk:compile:clean & npm run sdk:compile",
    "sdk:compile:clean": "rimraf build",
    "sdk:install:clean": "rimraf node_modules",
    "sdk:test": "echo \"Error: no test specified\" && exit 1",
    "sdk:compile": "tsc --build tsconfig.json tsconfig.esm.json tsconfig.esnext.json",
    "sdk:compile:esm": "tsc --build tsconfig.esm.json",
    "sdk:compile:esnext": "tsc --build tsconfig.esnext.json",
    "sdk:compile:cjs": "tsc --build tsconfig.json",
    "sdk:lint:fix": "eslint --fix ./src"
  },
  "files": [
    "build/esm/**/*.js",
    "build/esm/**/*.d.ts",
    "build/esnext/**/*.js",
    "build/esnext/**/*.d.ts",
    "build/src/**/*.js",
    "build/src/**/*.d.ts",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    "types": "./build/index.d.ts",
    "import": "./build/esm/index.js",
    "require": "./build/src/index.js",
    "default": "./build/src/index.js"
  },
  "author": "Embrace <support@embrace.io> (https://embrace.io/)",
  "license": "Apache-2.0",
  "dependencies": {
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-web": "0.45.0",
    "@opentelemetry/exporter-logs-otlp-http": "0.57.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.57.0",
    "@opentelemetry/exporter-trace-otlp-http": "0.57.0",
    "@opentelemetry/instrumentation": "0.57.0",
    "@opentelemetry/opentelemetry-browser-detector": "0.57.0",
    "@opentelemetry/otlp-exporter-base": "0.57.0",
    "@opentelemetry/propagator-b3": "1.30.0",
    "@opentelemetry/sdk-metrics": "1.30.0",
    "@opentelemetry/sdk-trace-web": "1.30.0",
    "@opentelemetry/web-common": "0.57.0",
    "shimmer": "^1.2.1",
    "uuid": "^11.1.0",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.12.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-require-extensions": "^0.1.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.18.2"
  }
}
